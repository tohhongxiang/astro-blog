{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "src/**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"paths": {
			"@/*": ["./src/*"]
		},
		"strictNullChecks": true,
		"noImplicitAny": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitThis": true,
	}
}
